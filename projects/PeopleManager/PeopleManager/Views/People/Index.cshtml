@model IList<PeopleManager.Models.Person>

@{
    ViewData["Title"] = "Home Page";
}

<table">
    <thead>
        <tr>
            <th>First name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var person in Model)
        {
            <partial name="PersonPartial" model="person" />
        }
    </tbody>
</table>
